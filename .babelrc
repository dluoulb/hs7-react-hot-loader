{
  "presets": [
    [
      "env",
      {
        "loose": true,
        "targets": {
          "browsers": [
            "last 2 chrome versions",
            "last 2 firefox versions",
            "last 2 safari versions",
            "last 2 edge versions"
          ]
        }
      }
    ],
    "react"
  ],
  "plugins": [
    ["transform-class-properties", { "loose": true }],
    "transform-object-rest-spread",
    "dynamic-import-node"
  ]
}
