{
  "extends": "airbnb",
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    },
  },
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["__REACT_HOT_LOADER__"] }],
    "no-console": ["error", { allow: ["error"] }],
    "strict": 0,
  },
  globals: {
    "__REACT_HOT_LOADER__": true
  }
}
