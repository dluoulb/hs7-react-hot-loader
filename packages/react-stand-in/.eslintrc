{
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
    sourceType: "module",
    ecmaVersion: 6
  },
  "parser": "babel-eslint",
  "rules": {
    "quotes": [2, "single"],
    "eol-last": [0],
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0],
    "no-alert": [0],
    "new-cap": [0],
    "no-empty": [0]
  }
}
